@font-face {
  font-family: Inconsolata;
  src: url("/Inconsolata.otf") format("opentype");
}
		
body {
  font-family: Inconsolata;
  color: White;
  padding: 0px;
  margin: 0px;
}

h1 {
  margin: 0px; padding:0px;
}

#bar {
  background: linear-gradient(to left, rgb(20,20,20), rgb(30,30,30));
  display: block;
  padding: 10px;
  position: fixed;
  width: 100%;
  height:40px;
  margin-top: 50%;
 wrapper {
  perspective: 550px;
  perspective-origin: 50% 150px;
  margin-top:70px;
}

#cube {
  position: relative;
  margin: 0 auto;
  height: 200px;
  width: 200px;
  transform-style: preserve-3d;
  transition: all 2s ease;
  z-index:1;
}

.face {
  position: absolute;
  height: 180px;
  width: 180px;
  padding: 10px;
  background: linear-gradient(to left, rgb(20,20,20), rgb(30,30,30));
}

#cube .one {
  transform: rotateX(90deg) translateZ(100px);
}

#cube .two {
  transform: translateZ(100px);
}

#cube .three {
  transform: rotateY(90deg) translateZ(100px);
}

#cube .four {
  transform: rotateY(180deg) translateZ(100px);
}

#cube .five {
  transform: rotateY(-90deg) translateZ(100px);
}

#cube .six {
  transform: rotateX(-90deg) translateZ(100px) rotate(180deg);
}
